def print_abacus(i):
	s = "00000*****"
	r = str(10 ** 10 + i)
	n = 1
	result = ""
	while n < 11:
		result = result + "#>>>|" + s[:10 - int(r[n])] + "   " + s[10 - int(r[n]):] + "|\n"
		n = n + 1
	print result

print "Abacus showing 0:"
print_abacus(0)
#>>>|00000***** | 00000***** | 00000***** | 00000***** | 00000***** |
#>>>|00000***** | 00000***** | 00000***** | 00000***** | 00000***** |
print "Abacus showing 12345678:"
print_abacus(12345678)
#>>>|00000*****   |
#>>>|00000*****   |
#>>>|00000****   *|XZ
#>>>|00000***   **|
#>>>|00000**   ***|
#>>>|00000*   ****|
#>>>|00000   *****|
#>>>|0000   0*****|
#>>>|000   00*****|
#>>>|00   000*****|
print "Abacus showing 1337:"
print_abacus(1337
#>>>|00000*****   |
#>>>|00000*****   |
#>>>|00000*****   |
#>>>|00000*****   |
#>>>|00000*****   |
#>>>|00000*****   |
#>>>|00000****   *|
#>>>|00000**   ***|
#>>>|00000**   ***|
#>>>|000   00*****|